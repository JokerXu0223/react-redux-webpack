{
  "name": "react-redux",
  "version": "1.0.0",
  "description": "react+redux+antd",
  "main": "index.js",
  "author": "JOKER XU",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"**/*.{js,jsx}\" --quiet",
    "start": "better-npm-run dev",
    "release": "webpack -p --progress --colors --config webpack.prod.js",
    "build": "npm run release"
  },
  "pre-commit": [
    "lint"
  ],
  "betterScripts": {
    "dev": {
      "command": "webpack-dev-server --progress --colors --config webpack.dev.js",
      "env": {
        "NODE_ENV": "development",
        "BABEL_ENV": "development",
        "BASE_API_ENDPOINT": "http:xx"
      }
    },
    "build:stage": {
      "command": "npm run release",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "production",
        "BASE_API_ENDPOINT": "http:qwe"
      }
    },
    "build:prod": {
      "command": "npm run release",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "production",
        "BASE_API_ENDPOINT": "http:12323"
      }
    }
  },
  "keywords": [],
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.6.5",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.0",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.6",
    "less": "^3.0.1",
    "less-loader": "^4.0.6",
    "less-vars-to-js": "^1.2.1",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.1",
    "pre-commit": "^1.2.2",
    "react-transform-hmr": "^1.0.4",
    "style-loader": "^0.20.2",
    "url-loader": "^1.0.1",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.2",
    "webpack-manifest-plugin": "^2.0.0-rc.2",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "antd": "^3.2.3",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "better-npm-run": "^0.1.0",
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.2.0",
    "react-addons-css-transition-group": "^15.6.2",
